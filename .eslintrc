{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "rules": {
    "no-console": 0,
    "comma-dangle": [2, "always-multiline"],
    "id-length": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "object-curly-spacing": 0,
    "quote-props": 0,
    "no-unused-expressions": 0,
    "max-len": [2, 140, 2, {
      "ignoreUrls": true,
      "ignoreComments": true
    }],
    "no-else-return": 0,
    "arrow-body-style": 0
  }
}
